pdown-menu slot=\"dropdown\">\n        <template v-if=\"isLogin\">\n          <el-dropdown-item @click.native=\"goToMine\">个人中心</el-dropdown-item>\n          <!--<el-dropdown-item divided>发布比赛</el-dropdown-item>-->\n          <!--<el-dropdown-item>发布消息</el-dropdown-item>-->\n          <!--<el-dropdown-item>发布评分</el-dropdown-item>-->\n          <!--<el-dropdown-item>后台管理</el-dropdown-item>-->\n          <el-dropdown-item divided @click.native=\"logOut\">退出登录</el-dropdown-item>\n        </template>\n        <template v-else>\n          <el-dropdown-item @click.native=\"goToLogin\">立即登录</el-dropdown-item>\n        </template>\n      </el-dropdown-menu>\n    </el-dropdown>\n  </div>\n</template>\n\n<script>\nimport {login} from \"@/utils/login\";\n\nexport default {\n  name: \"UserInfo\",\n  data() {\n    return {\n      msg: 5, //右上角消息数量\n      badgeHidden: false, //控制小圆圈是否显示，如果msg为0则值为true\n      isLogin: true, // 根据用户是否登录控制显示右上角列表选项\n    }\n  },\n  mounted() {\n    // 判断是否存在登录信息\n    this.isLogin = login(this.$route.path);\n  },\n  methods: {\n    goToLogin() {\n      this.$router.push(\"/login\");\n    },\n    goToMine() {\n      this.$router.push(\"/mine\");\n    },\n    logOut() {\n      sessionStorage.clear();\n      localStorage.clear();\n      this.$router.push(\"/login\");\n    }\n  }\n}\n</script>\n\n<style lang=\"less\" scoped>\n// 用户\n.user-info {\n  height: 48px;\n  background-clip: content-box;\n  padding: 6px 10px;\n  display: flex;\n  align-items: center;\n  justify-content: flex-end;\n\n  .item {\n    margin-top: 10px;\n    margin-right: 30px;\n  }\n}\n</style>"]}]}