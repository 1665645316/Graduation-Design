  <div class=\"list\">\r\n        <el-row>\r\n          <el-col v-for=\"(item) in lists\" :span=\"9\" :key=\"item.contestId\" :offset=\"2\" style=\"margin-bottom: 30px\">\r\n            <el-card :body-style=\"{ padding: '0' }\">\r\n              <router-link :to=\"{ name: 'find-detail', params: {contestId: item.contestId,name:item.name} }\">\r\n                <img :src=\"item.url\" class=\"image\" alt=\"\">\r\n              </router-link>\r\n              <div style=\"padding: 14px;\">\r\n                <span style=\"text-overflow: ellipsis;text-overflow: ellipsis;\">{{ item.contestTitle }}</span>\r\n                <div class=\"bottom clearfix\">\r\n                  <time class=\"time\">{{ item.regStartTime }}至{{ item.regEndTime }}</time>\r\n                  <el-button type=\"text\" class=\"button\">{{item.name}}</el-button>\r\n\r\n                </div>\r\n              </div>\r\n            </el-card>\r\n          </el-col>\r\n        </el-row>\r\n      </div>\r\n      <router-link class=\"more-link\" :to=\"{path:'/find'}\">\r\n        查看更多...\r\n      </router-link>\r\n    </el-card>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport {getRequest} from \"@/utils/api\";\r\n\r\nexport default {\r\n  name: \"competitionList\",\r\n  data() {\r\n    return {\r\n      page: '0',\r\n      lists: [{\r\n        contestId: '1',\r\n        contestTitle:'2021年度全国大学生算法设计编程挑战赛',\r\n        name: '德隆东墙',\r\n        url: 'https://pic.imgdb.cn/item/61e95d082ab3f51d918ed3d2.png',\r\n        regStartTime:'2022-02-01',\r\n        regEndTime:'2022-05-01',\r\n      }],\r\n    }\r\n  },\r\n  mounted() {\r\n    //判断是否为find页面\r\n    getRequest(\"/ContestsList/\"+this.page).then((resp) => {\r\n      const data = resp.data.data;\r\n      console.log(data);\r\n      if (data.length !== 4){\r\n        return false;\r\n      }else {\r\n        this.lists.push(data);\r\n        this.page+=4;\r\n      }\r\n    });\r\n  },\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.more-link {\r\n  paddin