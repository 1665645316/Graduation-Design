  <div style=\"padding: 20px 20%\">\n            <div class=\"title\">{{ title }}</div>\n            <div class=\"time\">{{ time }}</div>\n            <div class=\"text\">{{ text }}</div>\n            <div class=\"name\" style=\"float: right\">{{ name }}</div>\n          </div>\n        </el-card>\n      </el-col>\n    </el-row>\n  </div>\n</template>\n\n<script>\nimport {getRequest} from \"@/utils/api\";\n\nexport default {\n  name: \"Announcement\",\n  data() {\n    return {\n      id:0,\n      title: '',\n      time: '',\n      text: '',\n      name: '',\n    }\n  },\n  watch:{\n    $route(to,from){\n      console.log(to);\n      // this.id = to.params.messageId;\n      // return this.getData();\n    }\n  },\n  mounted() {\n    getRequest(\"/messages/announcement/\" + this.$route.params.messageId).then((res) => {\n      const data = res.data.data;\n      console.log(data);\n      console.log(data.recipient);\n      console.log(sessionStorage.uid);\n      if (data.recipient === 1 || data.recipient.toString() === sessionStorage.uid) {\n        this.title = data.title;\n        this.time = data.time;\n        this.text = data.text;\n        this.name = data.name;\n      } else {\n        this.$message.error(\"消息查询失败！\");\n      }\n\n\n    })\n  }\n}\n</script>\n\n<style scoped>\n.title {\n  text-align: center;\n  font-weight: bold;\n  font-size: 30px\n}\n\n.time {\n  margin: 20px 0;\n  text-align: center;\n  font-size: 14px;\n  color: #909399\n}\n\n.text ,.name{\n  font-size: 15px;\n  color: #606266\n}\n</style>"]}]}