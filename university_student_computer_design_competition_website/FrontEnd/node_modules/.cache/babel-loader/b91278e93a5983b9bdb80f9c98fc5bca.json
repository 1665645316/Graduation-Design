A,cAAA,KAAA,CAAA,OAAA,GAAA,QAAA;AACA,cAAA,KAAA,CAAA,SAAA,GAAA,KAAA;AACA;;AACA,YAAA,KAAA,CAAA,UAAA,CAAA,CAAA,EAAA,SAAA,GAAA,IAAA,CAAA,SAAA;AACA,YAAA,KAAA,CAAA,UAAA,CAAA,CAAA,EAAA,SAAA,GAAA,IAAA,CAAA,OAAA;AACA,WAdA,MAcA;AACA,YAAA,KAAA,CAAA,OAAA,CAAA,IAAA,CAAA,MAAA;AACA;AACA,SAnBA;AAoBA;AACA,KAzCA;;AA0CA;AACA;AACA;AACA,IAAA,MA7CA,oBA6CA;AAAA;;AACA,MAAA,UAAA,CAAA,gBAAA,KAAA,MAAA,CAAA,KAAA,CAAA,GAAA,GAAA,GAAA,GAAA,KAAA,SAAA,CAAA,CAAA,IAAA,CAAA,UAAA,IAAA,EAAA;AACA,YAAA,IAAA,CAAA,IAAA,CAAA,IAAA,KAAA,CAAA,EAAA;AACA,UAAA,MAAA,CAAA,QAAA,CAAA,OAAA,CAAA,OAAA;;AACA,UAAA,MAAA,CAAA,SAAA,GAAA,KAAA,CAFA,CAGA;;AACA,cAAA,GAAA,GAAA;AACA,YAAA,SAAA,EAAA,MAAA,CAAA,MAAA,CAAA,MAAA,CAAA,KAAA,CAAA,GAAA,CADA;AAEA,YAAA,KAAA,EAAA,QAFA;AAGA,YAAA,IAAA,EAAA,eAAA,MAAA,CAAA,YAHA;AAIA,YAAA,MAAA,EAAA,MAAA,CAAA;AAJA,WAAA;AAMA,UAAA,WAAA,CAAA,kBAAA,EAAA,GAAA,CAAA,CAAA,IAAA,CAAA,UAAA,GAAA,EAAA;AACA,gBAAA,GAAA,CAAA,IAAA,CAAA,IAAA,KAAA,CAAA,EAAA;AACA,cAAA,MAAA,CAAA,MAAA;AACA;AACA,WAJA;AAKA,SAfA,MAeA;AACA,UAAA,MAAA,CAAA,QAAA,CAAA,KAAA,CAAA,OAAA;AACA;AACA,OAnBA;AAoBA;AAlEA;AA9CA,CAAA","sourcesContent":["<template>\r\n  <div>\r\n    <el-row :gutter=\"20\">\r\n      <el-col :span=\"16\" :offset=\"4\">\r\n        <!--返回上页-->\r\n        <button class=\"btn\" @click=\"$router.go(-1);\" style=\"cursor: pointer;\"><i class=\"fa fa-circle-arrow-left\"></i>&nbsp;返回\r\n        </button>\r\n      </el-col>\r\n    </el-row>\r\n    <el-row :gutter=\"20\">\r\n      <el-col :span=\"12\" :offset=\"4\">\r\n        <!--左侧信息-->\r\n        <el-card :body-style=\"{ padding: '0' ,backgroundColor: 'rgb(251, 251, 251)'}\">\r\n          <div style=\"font-size: 24px;color: #303133;text-align: center;width: 100%;padding-top: 20px\">\r\n            {{ contestTitle }}\r\n          </div>\r\n          <el-divider></el-divider>\r\n          <mdEditor v-model=\"contestText\" :ishljs=\"true\" :subfield=\"false\" :toolbars=\"{}\" defaultOpen=\"preview\"\r\n                    style=\"min-height: 600px\" :toolbarsFlag=\"false\"/>\r\n        </el-card>\r\n      </el-col>\r\n      <!--右侧信息-->\r\n      <el-col :span=\"4\">\r\n        <el-card>\r\n          <div class=\"block\">\r\n            <el-timeline>\r\n              <el-timeline-item v-for=\"(activity, index) in activities\" :key=\"index\" :icon=\"activity.icon\"\r\n                                :type=\"activity.type\" :color=\"activity.color\" :size=\"activity.size\"\r\n                                :timestamp=\"activity.timestamp\">\r\n                {{ activity.content }}\r\n              </el-timeline-item>\r\n            </el-timeline>\r\n          </div>\r\n          <el-divider></el-divider>\r\n          <div class=\"text item\">\r\n            发布人：<samp class=\"itemRight\">{{ name }}</samp>\r\n          </div>\r\n          <div class=\"text item\">\r\n            所属组：<samp class=\"itemRight\">{{ groupName }}</samp>\r\n          </div>\r\n          <div style=\"text-align: center\" v-if=\"btnHidden\">\r\n            <el-button type=\"primary\" round @click=\"submit\">{{ btnText }}</el-button>\r\n          </div>\r\n          <div style=\"text-align: center\" v-else>\r\n            <el-button type=\"danger\" round disabled>{{ btnText }}</el-button>\r\n          </div>\r\n        </el-card>\r\n      </el-col>\r\n    </el-row>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport {getRequest, postRequest} from \"@/utils/api\";\r\nimport 'mavon-editor/dist/css/index.css'\r\n\r\nconst mdEditor = require('mavon-editor')\r\n\r\nexport default {\r\n  inject: ['reload'],                                 //注入App里的reload方法\r\n  components: {\r\n    'mdEditor': mdEditor.mavonEditor\r\n  },\r\n  data() {\r\n    return {\r\n      btnHidden: false,\r\n      btnText: '不可报名',\r\n      promulgator: 0,\r\n      contestTitle: '',\r\n      contestText: '',\r\n      contestId: '',\r\n      name: '',\r\n      groupName: '',\r\n      activities: [{\r\n        content: '报名开始',\r\n        timestamp: '2022-02-22 22:22',\r\n        size: 'large',\r\n        type: 'primary'\r\n      }, {\r\n        content: '报名结束',\r\n        timestamp: '2022-02-22 22:22',\r\n        size: 'large',\r\n        type: 'success'\r\n      }, {\r\n        content: '比赛开始',\r\n        timestamp: '2022-02-22 22:22',\r\n        size: 'large',\r\n        type: 'warning'\r\n      }, {\r\n        content: '比赛结束',\r\n        timestamp: '2022-02-22 22:22',\r\n        size: 'large',\r\n        type: 'danger'\r\n      }],\r\n    }\r\n  },\r\n  mounted() {\r\n    this.contestLoading();\r\n  },\r\n  watch: {\r\n    '$store.state.uid'() {\r\n      this.contestLoading();\r\n    }\r\n  },\r\n  methods: {\r\n    // 初始化\r\n    contestLoading() {\r\n      if (this.$store.state.uid !== undefined) {\r\n        this.contestId = this.$route.params.contestId;\r\n        // 查询当前用户是否已报名此比赛\r\n        getRequest(\"/scores/\" + this.$store.state.uid + \"/\" + this.contestId).then((res) => {\r\n          if (this.$store.state.gid === '2') {\r\n            if (res.data.data === 0) {\r\n              this.btnHidden = true;\r\n              this.btnText = '立即报名';\r\n            } else {\r\n              this.btnHidden = false;\r\n              this.btnText = '已报名';\r\n            }\r\n          } else {\r\n            this.btnHidden = false;\r\n          }\r\n        });\r\n        // 查询id为this.$route.params.contestId的比赛并显示\r\n        getRequest(\"/contest/\" + this.contestId).then((res) => {\r\n          const data = res.data.data;\r\n          if (data) {\r\n            document.title = this.contestTitle = data.contestTitle;\r\n            this.contestText = data.contestText;\r\n            this.name = data.name;\r\n            this.groupName = data.groupName;\r\n            this.promulgator = data.promulgator;\r\n            this.activities[0].timestamp = data.regStartTime;\r\n            this.activities[1].timestamp = data.regEndTime;\r\n            if (new Date() > new Date(data.regEndTime)) {\r\n              this.btnText = '报名时间已过';\r\n              this.btnHidden = false;\r\n            }\r\n            this.activities[2].timestamp = data.startTime;\r\n            this.activities[3].timestamp = data.endTime;\r\n          } else {\r\n            this.$router.push(\"/404\");\r\n          }\r\n        });\r\n      }\r\n    },\r\n    /**\r\n     * 点击报名按钮\r\n     */\r\n    submit() {\r\n      getRequest(\"/addScores/\" + this.$store.state.uid + \"/\" + this.contestId).then((resp) => {\r\n        if (resp.data.data === 1) {\r\n          this.$message.success(\"报名成功。\")\r\n          this.btnHidden = false;\r\n          // 发送一条消息给报名用户\r\n          const obj = {\r\n            recipient: Number(this.$store.state.uid),\r\n            title: '报名成功通知',\r\n            text: '恭喜您成功报名比赛：' + this.contestTitle,\r\n            sender: this.promulgator\r\n          };\r\n          postRequest(\"/messages/insert\", obj).then((res) => {\r\n            if (res.data.data === 1) {\r\n              this.reload();\r\n            }\r\n          })\r\n        } else {\r\n          this.$message.error(\"报名失败！\")\r\n        }\r\n      })\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"less\" scoped>\r\n.btn {\r\n  padding: 5px;\r\n  margin-bottom: 10px;\r\n  color: #515a6e;\r\n  background-color: transparent;\r\n  border-color: transparent;\r\n\r\n  &:hover {\r\n    color: #ff6464;\r\n  }\r\n}\r\n\r\n.text {\r\n  font-size: 16px;\r\n}\r\n\r\n.item {\r\n  padding: 0 20px 15px;\r\n}\r\n\r\n.box-card {\r\n  width: 480px;\r\n}\r\n\r\n.v-note-wrapper {\r\n  box-shadow: none !important;\r\n}\r\n\r\n.itemRight {\r\n  float: right;\r\n}\r\n</style>"],"sourceRoot":"src/views/layout/Main/Find"}]}