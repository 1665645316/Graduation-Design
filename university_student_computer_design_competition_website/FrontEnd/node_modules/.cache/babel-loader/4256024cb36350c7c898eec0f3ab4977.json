AAIA,MAAA,IAAA,EAAA;AAJA,KAAA;AAMA,GATA;AAUA,EAAA,QAAA,EAAA;AACA;AACA,IAAA,WAFA,yBAEA;AACA,UAAA,KAAA,MAAA,KAAA,CAAA,EAAA;AACA,eAAA,IAAA;AACA;AACA,KANA,CAMA;;AANA,GAVA;AAkBA,EAAA,OAlBA,qBAkBA;AACA;AACA,SAAA,OAAA,GAAA,KAAA,CAAA,KAAA,MAAA,CAAA,IAAA,CAAA,CAFA,CAGA;;AACA,SAAA,WAAA;AACA,GAvBA;AAwBA,EAAA,OAAA,EAAA;AACA;AACA,IAAA,WAFA,yBAEA;AAAA;;AACA,MAAA,KAAA,CAAA,CAAA,CAAA;AACA,MAAA,UAAA,CAAA,yBAAA,cAAA,CAAA,GAAA,CAAA,CAAA,IAAA,CAAA,UAAA,GAAA,EAAA;AACA,YAAA,IAAA,GAAA,GAAA,CAAA,IAAA,CAAA,IAAA;AACA,YAAA,MAAA,GAAA,CAAA;;AACA,aAAA,IAAA,CAAA,GAAA,CAAA,EAAA,CAAA,GAAA,IAAA,CAAA,MAAA,EAAA,CAAA,EAAA,EAAA;AACA,cAAA,CAAA,IAAA,CAAA,CAAA,CAAA,CAAA,KAAA,EAAA;AACA,YAAA,MAAA;AACA;AACA;;AACA,QAAA,KAAA,CAAA,MAAA,GAAA,MAAA;AACA,QAAA,KAAA,CAAA,IAAA,GAAA,IAAA;AACA,OAVA;AAUA,MAAA,KAAA,CAAA,CAAA,CAAA;AACA,KAfA;AAgBA;AACA,IAAA,MAjBA,oBAiBA;AACA,MAAA,cAAA,CAAA,KAAA;AACA,MAAA,YAAA,CAAA,KAAA;AACA,WAAA,OAAA,CAAA,IAAA,CAAA,QAAA;AACA,KArBA;AAsBA;AACA,IAAA,UAvBA,sBAuBA,GAvBA,EAuBA;AACA,WAAA,WAAA;AACA,WAAA,MAAA,GAAA,KAAA;AACA,WAAA,OAAA,CAAA,IAAA,CAAA;AAAA,QAAA,IAAA,EAAA,qBAAA;AAAA,QAAA,MAAA,EAAA;AAAA,UAAA,SAAA,EAAA,GAAA,CAAA;AAAA;AAAA,OAAA;AACA,KA3BA,CA4BA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAnCA;AAxBA,CAAA","sourcesContent":["<template>\n  <div class=\"user-info\">\n    <el-badge :value=\"unread\" :max=\"99\" :hidden=\"badgeHidden\" class=\"item\">\n      <el-button size=\"small\" icon=\"fa fa-bell\" @click=\"drawer = true\">消息</el-button>\n    </el-badge>\n    <el-dropdown>\n      <div class=\"el-dropdown-link\">\n        <el-link>\n          <el-avatar :size=\"48\" src=\"https://cube.elemecdn.com/0/88/03b0d39583f48206768a7534e55bcpng.png\"></el-avatar>\n        </el-link>\n      </div>\n      <el-dropdown-menu slot=\"dropdown\">\n        <template v-if=\"isLogin\">\n          <el-dropdown-item @click.native=\"$router.push('/mine')\">个人中心</el-dropdown-item>\n          <!--<el-dropdown-item divided>发布比赛</el-dropdown-item>-->\n          <!--<el-dropdown-item>发布消息</el-dropdown-item>-->\n          <!--<el-dropdown-item>发布评分</el-dropdown-item>-->\n          <!--<el-dropdown-item>后台管理</el-dropdown-item>-->\n          <el-dropdown-item divided @click.native=\"logOut\">退出登录</el-dropdown-item>\n        </template>\n        <template v-else>\n          <el-dropdown-item @click.native=\"$router.push('/login')\">立即登录</el-dropdown-item>\n        </template>\n      </el-dropdown-menu>\n    </el-dropdown>\n    <!--消息抽屉-->\n    <div>\n      <el-drawer\n          :append-to-body=\"true\"\n          :modal-append-to-body=\"false\"\n          :visible.sync=\"drawer\"\n          direction=\"rtl\"\n          size=\"50%\">\n        <el-table :data=\"data\" :current-row-key=\"data.messageId\" fit @row-click=\"clickTable\">\n          <el-table-column property=\"title\" label=\"标题\" align=\"center\"></el-table-column>\n          <el-table-column property=\"time\" label=\"时间\" align=\"center\"></el-table-column>\n          <el-table-column property=\"name\" label=\"其他\" align=\"center\" width=\"100%\">\n            <template v-slot=\"scope\">\n              <el-tag :type=\"scope.row.name === '管理员' ? 'danger' : ''\" disable-transitions>\n                {{ scope.row.name }}\n              </el-tag>\n            </template>\n          </el-table-column>\n          <el-table-column property=\"name\" label=\"状态\" align=\"center\" width=\"100%\">\n            <template v-slot=\"scope\">\n              <el-tag :type=\"scope.row.state ? 'info' : 'warning'\" disable-transitions>\n                {{ scope.row.state ? '已读' : '未读' }}\n              </el-tag>\n            </template>\n          </el-table-column>\n        </el-table>\n      </el-drawer>\n    </div>\n  </div>\n\n</template>\n\n<script>\nimport {login} from \"@/utils/login\";\nimport {getRequest} from \"@/utils/api\";\n\nexport default {\n  name: \"UserInfo\",\n  data() {\n    return {\n      unread: 5, //右上角未读消息数量\n      isLogin: false, // 根据用户是否登录控制显示右上角列表选项\n      drawer: false,\n      data: []\n    }\n  },\n  computed: {\n    // 判断未读消息如果为0则隐藏小红圈\n    badgeHidden() {\n      if (this.unread === 0) {\n        return true;\n      }\n    }, //控制小圆圈是否显示，如果msg为0则值为true\n  },\n  mounted() {\n    // 判断是否存在登录信息\n    this.isLogin = login(this.$route.path);\n    // 获取未读消息\n    this.loadMessage();\n  },\n  methods: {\n    // 加载消息\n    loadMessage() {\n      alert(1);\n      getRequest(\"/messages/recipient/\" + sessionStorage.uid).then((res => {\n        const data = res.data.data;\n        let unread = 0;\n        for (let i = 0; i < data.length; i++) {\n          if (!data[i].state) {\n            unread++;\n          }\n        }\n        this.unread = unread;\n        this.data = data;\n      }));alert(2);\n    },\n    // 退出登录\n    logOut() {\n      sessionStorage.clear();\n      localStorage.clear();\n      this.$router.push(\"/login\");\n    },\n    // 点击表格一列进行跳转\n    clickTable(row) {\n      this.loadMessage();\n      this.drawer = false;\n      this.$router.push({name: 'announcement-detail', params: {messageId: row.messageId}});\n    },\n    // // 已读消息更改样式\n    // tableRowClass({row}){\n    //   if (row.state) {\n    //     return {'background-color':'#f9f9f9'};\n    //   } else{\n    //     return {'background-color':'#fff'};\n    //   }\n    // }\n  }\n}\n</script>\n\n<style lang=\"less\" scoped>\n// 用户\n.user-info {\n  height: 48px;\n  background-clip: content-box;\n  padding: 6px 10px;\n  display: flex;\n  align-items: center;\n  justify-content: flex-end;\n\n  .item {\n    margin-top: 10px;\n    margin-right: 30px;\n  }\n}\n</style>"],"sourceRoot":"src/views/layout/Header"}]}