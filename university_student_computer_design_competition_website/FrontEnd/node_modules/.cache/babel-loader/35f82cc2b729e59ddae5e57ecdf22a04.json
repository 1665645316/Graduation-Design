(res) => {\n      const data = res.data.data;\n      console.log(data);\n      console.log(data.recipient);\n      console.log(sessionStorage.uid);\n      if (data.recipient === 1 || data.recipient.toString() === sessionStorage.uid) {\n        this.title = data.title;\n        this.time = data.time;\n        this.text = data.text;\n        this.name = data.name;\n      } else {\n        this.$message.error(\"消息查询失败！\");\n      }\n\n\n    })\n  }\n}\n</script>\n\n<style scoped>\n.title {\n  text-align: center;\n  font-weight: bold;\n  font-size: 30px\n}\n\n.time {\n  margin: 20px 0;\n  text-align: center;\n  font-size: 14px;\n  color: #909399\n}\n\n.text ,.name{\n  font-size: 15px;\n  color: #606266\n}\n</style>"],"sourceRoot":"src/views/layout/Main/Announcements"}]}