;AAJA,KAAA;AAMA,GATA;AAUA,EAAA,QAAA,EAAA;AACA;AACA,IAAA,OAFA,qBAEA;AACA,aAAA,KAAA,MAAA,CAAA,KAAA,CAAA,OAAA;AACA;AAJA,GAVA;AAgBA,EAAA,OAhBA,qBAgBA;AACA;AACA,SAAA,eAAA;AACA,GAnBA;AAoBA,EAAA,OAAA,EAAA;AACA,IAAA,aADA,2BACA;AACA,WAAA,eAAA;AACA,WAAA,MAAA,GAAA,IAAA;AACA,KAJA;AAKA,IAAA,eALA,6BAKA;AAAA;;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,QAAA;;AACA,UAAA,KAAA,MAAA,CAAA,KAAA,CAAA,GAAA,EAAA;AACA,QAAA,UAAA,CAAA,yBAAA,KAAA,MAAA,CAAA,KAAA,CAAA,GAAA,CAAA,CAAA,IAAA,CAAA,UAAA,GAAA,EAAA;AACA,cAAA,IAAA,GAAA,GAAA,CAAA,IAAA,CAAA,IAAA;AACA,UAAA,OAAA,CAAA,GAAA,CAAA,IAAA;AACA,cAAA,MAAA,GAAA,CAAA;;AACA,eAAA,IAAA,CAAA,GAAA,CAAA,EAAA,CAAA,GAAA,IAAA,CAAA,MAAA,EAAA,CAAA,EAAA,EAAA;AACA,gBAAA,CAAA,IAAA,CAAA,CAAA,CAAA,CAAA,KAAA,EAAA;AACA,cAAA,MAAA;AACA;AACA;;AACA,UAAA,OAAA,CAAA,GAAA,CAAA,MAAA;AACA,UAAA,KAAA,CAAA,WAAA,GAAA,MAAA,KAAA,CAAA;AACA,UAAA,KAAA,CAAA,MAAA,GAAA,MAAA;AACA,UAAA,KAAA,CAAA,IAAA,GAAA,IAAA;AACA,SAbA;AAcA;AACA,KAvBA;AAwBA;AACA,IAAA,MAzBA,oBAyBA;AACA,WAAA,MAAA,CAAA,KAAA,CAAA,OAAA,GAAA,KAAA;AACA,WAAA,MAAA,CAAA,KAAA,CAAA,GAAA,GAAA,KAAA;AACA,WAAA,MAAA,CAAA,KAAA,CAAA,IAAA,GAAA,KAAA;AACA,WAAA,MAAA,CAAA,KAAA,CAAA,GAAA,GAAA,KAAA;AACA,MAAA,YAAA,CAAA,KAAA;AACA,WAAA,OAAA,CAAA,IAAA,CAAA,QAAA;AACA,KAhCA;AAiCA;AACA,IAAA,UAlCA,sBAkCA,GAlCA,EAkCA;AACA,UAAA,EAAA,GAAA,GAAA,CAAA,SAAA,CADA,CAEA;;AACA,WAAA,MAAA,GAAA,KAAA,CAHA,CAIA;;AACA,WAAA,OAAA,CAAA,IAAA,CAAA;AAAA,QAAA,IAAA,EAAA,iBAAA;AAAA,QAAA,MAAA,EAAA;AAAA,UAAA,SAAA,EAAA;AAAA;AAAA,OAAA,EALA,CAMA;;AACA,MAAA,UAAA,CAAA,iBAAA,EAAA;AAAA,QAAA,SAAA,EAAA,EAAA;AAAA,QAAA,KAAA,EAAA;AAAA,OAAA,CAAA,CAPA,CAQA;;AACA,WAAA,eAAA;AACA;AA5CA;AApBA,CAAA","sourcesContent":["<template>\n  <div class=\"user-info\">\n    <el-badge :value=\"unread\" :max=\"99\" :hidden=\"badgeHidden\" class=\"item\">\n      <el-button size=\"small\" icon=\"fa fa-bell\" @click=\"drawerControl\">消息</el-button>\n    </el-badge>\n    <el-dropdown>\n      <div class=\"el-dropdown-link\">\n        <el-link>\n          <el-avatar :size=\"48\" src=\"https://cube.elemecdn.com/0/88/03b0d39583f48206768a7534e55bcpng.png\"></el-avatar>\n        </el-link>\n      </div>\n      <el-dropdown-menu slot=\"dropdown\">\n        <template v-if=\"isLogin\">\n          <el-dropdown-item @click.native=\"$router.push('/mine')\">个人中心</el-dropdown-item>\n          <!--<el-dropdown-item divided>发布比赛</el-dropdown-item>-->\n          <!--<el-dropdown-item>发布消息</el-dropdown-item>-->\n          <!--<el-dropdown-item>发布评分</el-dropdown-item>-->\n          <!--<el-dropdown-item>后台管理</el-dropdown-item>-->\n          <el-dropdown-item divided @click.native=\"logOut\">退出登录</el-dropdown-item>\n        </template>\n        <template v-else>\n          <el-dropdown-item @click.native=\"$router.push('/login')\">立即登录</el-dropdown-item>\n        </template>\n      </el-dropdown-menu>\n    </el-dropdown>\n    <!--消息抽屉-->\n    <div>\n      <el-drawer\n          :append-to-body=\"true\"\n          :modal-append-to-body=\"false\"\n          :visible.sync=\"drawer\"\n          direction=\"rtl\"\n          size=\"50%\">\n        <el-table :data=\"data\" :current-row-key=\"data.messageId\" fit @row-click=\"clickTable\">\n          <el-table-column property=\"title\" label=\"标题\" align=\"center\"></el-table-column>\n          <el-table-column property=\"time\" label=\"时间\" align=\"center\"></el-table-column>\n          <el-table-column property=\"name\" label=\"发布人\" align=\"center\" width=\"100%\">\n            <template v-slot=\"scope\">\n              <el-tag :type=\"scope.row.name === '管理员' ? 'danger' : ''\" disable-transitions>\n                {{ scope.row.name }}\n              </el-tag>\n            </template>\n          </el-table-column>\n          <el-table-column property=\"name\" label=\"状态\" align=\"center\" width=\"100%\">\n            <template v-slot=\"scope\">\n              <el-tag :type=\"scope.row.state ? 'info' : 'warning'\" disable-transitions>\n                {{ scope.row.state ? '已读' : '未读' }}\n              </el-tag>\n            </template>\n          </el-table-column>\n        </el-table>\n      </el-drawer>\n    </div>\n  </div>\n\n</template>\n\n<script>\nimport {getRequest, putRequest} from \"@/utils/api\";\n\nexport default {\n  name: \"UserInfo\",\n  data() {\n    return {\n      unread: 0, //右上角未读消息数量\n      badgeHidden : true,\n      drawer: false,\n      data: []\n    }\n  },\n  computed:{\n    // 判断是否存在登录信息\n    isLogin(){\n      return this.$store.state.isLogin;\n    }\n  },\n  mounted() {\n    // 获取未读消息\n    this.userInfoLoading();\n  },\n  methods: {\n    drawerControl() {\n      this.userInfoLoading();\n      this.drawer = true;\n    },\n    userInfoLoading(){\n      console.log(\"执行消息获取\");\n      if(this.$store.state.uid){\n        getRequest(\"/messages/recipient/\" + this.$store.state.uid).then((res => {\n          const data = res.data.data;\n          console.log(data);\n          let unread = 0;\n          for (let i = 0; i < data.length; i++) {\n            if (!data[i].state) {\n              unread++;\n            }\n          }\n          console.log(unread);\n          this.badgeHidden = unread === 0;\n          this.unread = unread;\n          this.data = data;\n        }));\n      }\n    },\n    // 退出登录\n    logOut() {\n      this.$store.state.isLogin = false;\n      this.$store.state.uid = false;\n      this.$store.state.name = false;\n      this.$store.state.gid = false;\n      localStorage.clear();\n      this.$router.push(\"/login\");\n    },\n    // 点击表格一列进行跳转\n    clickTable(row) {\n      let id = row.messageId;\n      // 隐藏抽屉\n      this.drawer = false;\n      // 跳转页面\n      this.$router.push({name: 'messages-detail', params: {messageId: id}});\n      // 未读变已读\n      putRequest(\"/messages/state\",{messageId:id,state:true});\n      // 修改data数据\n      this.userInfoLoading();\n    }\n  }\n}\n</script>\n\n<style lang=\"less\" scoped>\n// 用户\n.user-info {\n  height: 48px;\n  background-clip: content-box;\n  padding: 6px 10px;\n  display: flex;\n  align-items: center;\n  justify-content: flex-end;\n\n  .item {\n    margin-top: 10px;\n    margin-right: 30px;\n  }\n}\n</style>"],"sourceRoot":"src/views/layout/Header"}]}